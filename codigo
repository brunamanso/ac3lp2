def incluir_novo_nome(agenda, nome, telefone):
    if nome not in agenda:
        lista=[telefone]
        agenda[nome] = lista



def excluir_nome(agenda, nome):
    if nome in agenda:
        del agenda[nome]


def consultar_telefone(agenda, nome):
   if nome in agenda:
       return agenda[nome]


def incluir_telefone(agenda, nome, telefone):
    if nome in agenda:
        agenda[nome] = telefone

    
def excluir_telefone(agenda, nome, telefone):
   if nome in agenda:
       del agenda[nome(telefone)]



    
    
agenda = {}
incluir_novo_nome (agenda,'pablovitoria', 123456)
incluir_novo_nome (agenda,'pabuvitoria', 123456)
incluir_novo_nome (agenda,'pablovituria', 123456)
print(agenda)
